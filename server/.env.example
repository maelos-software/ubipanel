# InfluxDB Connection
# URL of your InfluxDB 1.x server
INFLUX_URL=http://localhost:8086

# Database name (unpoller default)
INFLUX_DB=unpoller

# InfluxDB credentials
INFLUX_USER=unpoller
INFLUX_PASS=your_password_here

# Proxy Server Port
# In development, this is overridden by DEV_PROXY_PORT in .env.development
# In production, set this to your desired port
PORT=3001

# Site Configuration
# Name displayed in the dashboard sidebar
SITE_NAME=My Network
